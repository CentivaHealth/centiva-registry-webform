{
	"name": "centiva-registry-webform",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 8080",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"create-js-ts-from-proto-add-info-hash": "pbjs -t static-module src/proto/api/provider_add_info_hash.proto -o src/models/proto/provider-add-info-hash.js && pbts --no-comments src/models/proto/provider-add-info-hash.js -o src/models/proto/provider-add-info-hash.d.ts",
		"create-js-ts-from-proto-error-response": "pbjs -t static-module src/proto/api/error_response.proto -o src/models/proto/error-response.js && pbts --no-comments src/models/proto/error-response.js -o src/models/proto/error-response.d.ts",
		"postinstall": "cd  src/proto/api; protoc --plugin=\"protoc-gen-ts=../../../node_modules/.bin/protoc-gen-ts\" --js_out=\"import_style=commonjs,binary:../../models/proto\" --ts_out=\"service=false:../../models/proto\" provider_add_info_hash.proto error_response.proto;",
		"build:prod": "ng build --prod",
		"test:ci": "ng test --no-watch --no-progress --browsers ChromeHeadlessCI",
		"e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^9.1.11",
		"@angular/cdk": "^9.2.4",
		"@angular/common": "~9.1.11",
		"@angular/compiler": "~9.1.11",
		"@angular/core": "~9.1.11",
		"@angular/fire": "^6.0.0",
		"@angular/forms": "~9.1.11",
		"@angular/platform-browser": "~9.1.11",
		"@angular/platform-browser-dynamic": "~9.1.11",
		"@angular/router": "~9.1.11",
		"@firebase/app": "^0.6.5",
		"@firebase/auth": "^0.14.6",
		"@firebase/auth-types": "^0.10.1",
		"@firebase/firestore": "^1.15.1",
		"@improbable-eng/grpc-web": "^0.12.0",
		"@types/google-protobuf": "^3.7.2",
		"angularx-qrcode": "^2.3.4",
		"firebase": "^7.15.1",
		"google-protobuf": "^3.12.2",
		"jspdf": "^1.5.3",
		"moment": "^2.27.0",
		"ng-pick-datetime": "^7.0.0",
		"ngx-mask": "^9.1.2",
		"ngx-moment": "^4.0.1",
		"ngx-toastr": "^12.1.0",
		"rxjs": "~6.5.4",
		"sha3": "^2.1.2",
		"ts-protoc-gen": "^0.12.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.901.8",
		"@angular/cli": "~9.1.8",
		"@angular/compiler-cli": "~9.1.11",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.12.47",
		"codelyzer": "^5.1.2",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-angular": "^0.5.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-formatter-summary": "^1.0.4",
		"eslint-html-reporter": "^0.7.4",
		"eslint-plugin-angular": "^4.0.1",
		"eslint-plugin-import": "^2.21.2",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-typescript": "^0.14.0",
		"eslint-restricted-globals": "^0.2.0",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~2.1.0",
		"karma-jasmine": "~3.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"prettier": "^2.0.5",
		"protractor": "~7.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~3.8.3",
		"typescript-eslint-parser": "^22.0.0",
		"@angular-devkit/architect": "~0.900",
		"firebase-tools": "^8.0.0",
		"fuzzy": "^0.1.3",
		"inquirer": "^6.2.2",
		"inquirer-autocomplete-prompt": "^1.0.1"
	}
}
